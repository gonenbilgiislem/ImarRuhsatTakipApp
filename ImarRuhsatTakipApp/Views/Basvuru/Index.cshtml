@model IEnumerable<ImarRuhsatTakipApp.Models.Basvuru>
	
	@{
		ViewBag.Title = "Index";
			}
	
	<br/>
	
	<br>
<div class="row-fluid">
<!-- block -->
<div class="block">
<div class="navbar navbar-inner block-header">
<div class="muted pull-left">Başvuru</div>
</div>
<div class="block-content collapse in">
<div class="span12">
<div class="table-toolbar">
<div class="btn-group">
<a href="@Url.Action("Create","Basvuru")" button class="btn btn-success"><i class="icon-plus icon-white" title="Kayıt Ekle"></i></a>
</div>
<div class="btn-group pull-right">
<button data-toggle="dropdown" class="btn dropdown-toggle">Araçlar <span class="caret"></span></button>
<ul class="dropdown-menu">
<li><a href="#">Yazdır</a></li>
<li><a href="#">PDF Kaydet</a></li>
<li><a href="#">Excele Kaydet</a></li>
</ul>
</div>
</div>


<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered pre-scrollable" id="example2">
<thead>
<tr>
		        <th>
		@Html.DisplayNameFor(model => model.Referans_kod)
        </th>
				        <th>
		@Html.DisplayNameFor(model => model.Mal_Sahibi_Ad_Soyad)
        </th>
				        <th>
		@Html.DisplayNameFor(model => model.Aciklama)
        </th>
				        <th>
		@Html.DisplayNameFor(model => model.Dilekce_Tarihi)
        </th>
				        <th>
		@Html.DisplayNameFor(model => model.Basvuru_Turleri.Basvuru_Tur_Ad)
        </th>
				        <th>
		@Html.DisplayNameFor(model => model.Durumlar.Durum_Aciklama)
        </th>
				        <th>
		@Html.DisplayNameFor(model => model.Kullanicilar.Kullanici_Ad_Soyad)
        </th>
				        <th>
		@Html.DisplayNameFor(model => model.Muteahhit.Mutheahhit_Ad_Soyad)
        </th>
				        <th>
		@Html.DisplayNameFor(model => model.Talep_Tipleri.Talep_Tip_Ad)
        </th>
				        <th>
		@Html.DisplayNameFor(model => model.Yapi_Denetim_Firmasi.Yapi_Denetim_Adi)
        </th>
		<th></th>
</tr>
</thead>
<tbody>
@foreach (var item in Model) {
    <tr>
							<td>
            @Html.DisplayFor(modelItem => item.Referans_kod)
			</td>
									<td>
            @Html.DisplayFor(modelItem => item.Mal_Sahibi_Ad_Soyad)
			</td>
									<td>
            @Html.DisplayFor(modelItem => item.Aciklama)
			</td>
									<td>
            @Html.DisplayFor(modelItem => item.Dilekce_Tarihi)
			</td>
									<td>
            @Html.DisplayFor(modelItem => item.Basvuru_Turleri.Basvuru_Tur_Ad)
			</td>
									<td>
            @Html.DisplayFor(modelItem => item.Durumlar.Durum_Aciklama)
			</td>
									<td>
            @Html.DisplayFor(modelItem => item.Kullanicilar.Kullanici_Ad_Soyad)
			</td>
									<td>
            @Html.DisplayFor(modelItem => item.Muteahhit.Mutheahhit_Ad_Soyad)
			</td>
									<td>
            @Html.DisplayFor(modelItem => item.Talep_Tipleri.Talep_Tip_Ad)
			</td>
									<td>
            @Html.DisplayFor(modelItem => item.Yapi_Denetim_Firmasi.Yapi_Denetim_Adi)
			</td>
					<td>
		<a href="@Url.Action("Edit","Basvuru",new {id = item.Id })" button class="btn btn-info"> <i class="icon-pencil icon-white" title="Kaydı Düzenle"></i></a>
        <a href="@Url.Action("Delete","Basvuru",new {id = item.Id })" button class="btn btn-danger"> <i class="icon-remove icon-white" title="Kaydı Sil"></i></a>
		</td>
		    </tr>
}
</tbody>
</table>
</div>
</div>
</div>
</div>

